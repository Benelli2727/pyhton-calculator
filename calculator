n = int(float("To add press(1), to subtract press(2), to multiply press(3), to divide press(4)")
num1 = float(input("Enter the first number"))
num2 = float(input("Enter the second number"))
if(n==1):
  print(num1+num2)

if(n==2):
  print(num1-num2)

if(n==3):
  print(num1*num2)

if(n==4):
  print(num1/num2)
